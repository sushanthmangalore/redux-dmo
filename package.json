{
  "name": "redux-demo",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore '**/__tests__/' --copy-files --source-maps",
    "start": "webpack-dev-server -d --inline --port 9080 --host 0.0.0.0",
    "test": "jest --no-cache",
    "eslint": "eslint -f html -o report/eslintReport.html --ext .jsx,.js src",
    "smoketest": "testcafe 'chrome:headless' smoketest.js",
    "prepublish": "npm run build"
  },
  "jest": {
    "automock": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "testRegex": "/src/.*(test|spec)\\.jsx?$",
    "verbose": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ],
    "timers": "fake",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/__tests__"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-devtools-extension": "^1.0.0",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-jest": "14.0.1",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "colors": "1.1.2",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jest-cli": "18.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-hot-middleware": "2.10.0",
    "testcafe": "*"
  }
}
